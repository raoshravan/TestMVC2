@model IEnumerable<MvcDemo.DataDb.AllEmployeeswithDp_Result>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div style="font-family:Arial">
    <h2>Employee List</h2>
    <ul>
        @foreach (var emp in Model)
        {
            <li>
                 @Html.ActionLink(emp.Name, "Details", new { id = emp.EmployeeId })          
            </li>            // <tr><td>@emp.EmployeeId</td><td>@emp.Name</td><td>@emp.City</td><td>@emp.Gender</td></tr>
        }
    </ul>
    @Html.ActionLink("Back to Department List", "Index","Department")
</div>
